<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <CustomAction Id="1ecd4af2-58f7-402a-bf86-a23c448aec50.RibbonButton"
                RegistrationType="List"
                RegistrationId="{$ListId:Shared%20Documents;}"
                Location="CommandUI.Ribbon"
                Sequence="10001"
                Title="Invoke &apos;RibbonButton&apos; action">
    <CommandUIExtension>
      <!-- 
      Update the UI definitions below with the controls and the command actions
      that you want to enable for the custom action.
      -->
      <CommandUIDefinitions>
        <CommandUIDefinition Location="Ribbon.Library.CustomizeLibrary.Controls._children">
          <Button Id="Ribbon.Library.CustomizeLibrary.RibbonButtonButton"
                  Alt="SPFile"
                  Sequence="100"
                  Command="Invoke_RibbonButtonButtonRequest"
                  LabelText="SPFile"
                  TemplateAlias="o1"
                  Image32by32="data:image/png;base64,"
                  Image16by16="data:image/png;base64," />
        </CommandUIDefinition>
      </CommandUIDefinitions>
      <CommandUIHandlers>
        <CommandUIHandler Command="Invoke_RibbonButtonButtonRequest"
                          CommandAction="~appWebUrl/Pages/Default.aspx?{StandardTokens}&amp;SPListItemId={SelectedItemId}&amp;SPListId={SelectedListId}"/>
      </CommandUIHandlers>
    </CommandUIExtension >
  </CustomAction>
</Elements>
